<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.a.com"
	xmlns:b="http://www.b.com"
	targetNamespace="http://www.a.com"
>

<xs:import namespace="
http://www.b.com" schemaLocation="b.xsd"></xs:import>

<xs:element name="A">
	<xs:complexType>
		<xs:sequence>
			<xs:sequence>
				<xs:element ref="b:B"></xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
</xs:element>

</xs:schema>
